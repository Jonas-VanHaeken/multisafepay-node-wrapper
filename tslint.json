{
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["./src/typings", "./__stubs__", "./__mocks__"]
  },
  "rules": {
    "quotemark": [true, "single"],
    "no-console": false,
    "no-null-keyword": true,
    "class-name": true,
    "no-var-keyword": true
  }
}
